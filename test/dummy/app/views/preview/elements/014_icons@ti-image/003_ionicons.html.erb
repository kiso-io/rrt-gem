<div class="container-fluid">
  <div class="row">
    <% IoniconsMetadata.all.each do |fa| %>
      <div class='col-12 col-md-3'>
        <div class='card' style="min-height: 190px;">
          <div class="card-body d-flex flex-column align-items-center justify-content-center inspectable">
            <span style='font-size: 48px;'><%= ionicon_icon_tag fa.id %></span>
            <p class="text-center"><%= fa.name %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
